{
    "name": "laravel-notification-channels/evernote",
    "description": "Evernote Notifications driver",
    "homepage": "https://github.com/laravel-notification-channels/evernote",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com",
            "homepage": "http://www.marcelpociot.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "evernote/evernote-cloud-sdk-php": "dev-master#bf3024b",
        "illuminate/notifications": "^5.3@dev",
        "illuminate/support": "^5.3@dev",
        "illuminate/events": "^5.3@dev"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*",
        "orchestra/testbench": "^3.3@dev"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Evernote\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Evernote\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
